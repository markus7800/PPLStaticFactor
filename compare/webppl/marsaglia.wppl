

var marsaglia = function (s, x) {
    if (s > 1) {
        var x = uniform(-1, 1)
        var y = uniform(-1, 1)
        var new_s = x*x + y*y
        return marsaglia(new_s, x)
    } else {
        return x * Math.sqrt(-2 * Math.log(s) / s)
    }
}

var model_rec = function() {
    return marsaglia(2, 0.)
}
